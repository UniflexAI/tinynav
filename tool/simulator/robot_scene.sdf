                            <size>0.05 0.05 0.05</size>
                        </box>
                    </geometry>
                </collision>
                <sensor name="infra2" type="camera">
                    <camera>
                        <horizontal_fov>1.5708</horizontal_fov>
                        <image>
                            <width>848</width>
                            <height>480</height>
                            <format>L8</format>
                        </image>
                        <clip>
                            <near>0.1</near>
                            <far>100</far>
                        </clip>
                    </camera>
                    <always_on>1</always_on>
                    <update_rate>30</update_rate>
                    <visualize>true</visualize>
                    <topic>/camera/camera/infra2/image_rect_raw</topic>
                </sensor>
            </link>
            
            <link name='left_wheel'>
                <pose relative_to="chassis">-0.5 0.6 0 -1.5707 0 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
