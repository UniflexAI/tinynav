        <!-- Robot Model -->
        <model name='vehicle_blue' canonical_link='chassis'>
            <plugin
                filename="gz-sim-diff-drive-system"
                name="gz::sim::systems::DiffDrive">
                <left_joint>left_wheel_joint</left_joint>
                <right_joint>right_wheel_joint</right_joint>
                <wheel_separation>1.2</wheel_separation>
                <wheel_radius>0.4</wheel_radius>
                <odom_publish_frequency>1</odom_publish_frequency>
                <topic>cmd_vel</topic>
            </plugin>

            <pose relative_to='world'>0 0 0 0 0 0</pose>
            <link name='chassis'>

                <sensor name="imu_sensor" type="imu">
                    <always_on>1</always_on>
                    <update_rate>200</update_rate>
                    <visualize>true</visualize>
                    <topic>/camera/camera/accel/sample</topic>
                    <!-- Direct camera coordinate system -->
                    <!-- Camera: x=right, y=down, z=forward -->
                    <!-- This pose directly aligns with camera coordinates -->
                    <pose>1.0 0 0.4 1.5708 3.14159 -1.5708</pose>
                </sensor>

                <pose relative_to='__model__'>0.5 0 0.4 0 0 0</pose>
                <inertial> <!--inertial properties of the link mass, inertia matix-->
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>2.0 1.0 0.5</size>
                        </box>
                    </geometry>
                    <!--let's add color to our link-->
                    <material>
                        <ambient>0.0 0.0 1.0 1</ambient>
                        <diffuse>0.0 0.0 1.0 1</diffuse>
                        <specular>0.0 0.0 1.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                        <geometry>
                            <box>
                                <size>2.0 1.0 0.5</size>
                            </box>
                        </geometry>
                </collision>
            </link>
            
            <!-- Infra1 Camera Link (Left) -->
            <link name='infra1_link'>
                <pose relative_to="chassis">1.0 0.0255 0.4 0 0 0</pose>
                <inertial>
                    <mass>0.1</mass>
                    <inertia>
                        <ixx>0.001</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.001</iyy>
                        <iyz>0</iyz>
                        <izz>0.001</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.05 0.05 0.05</size>
                        </box>
                    </geometry>
                    <material>
                        <ambient>0.2 0.2 0.2 1</ambient>
                        <diffuse>0.2 0.2 0.2 1</diffuse>
                        <specular>0.2 0.2 0.2 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <box>
                            <size>0.05 0.05 0.05</size>
                        </box>
                    </geometry>
                </collision>
                <sensor name="infra1" type="camera">
                    <camera>
                        <horizontal_fov>1.5708</horizontal_fov>
                        <image>
                            <width>848</width>
                            <height>480</height>
                            <format>L8</format>
                        </image>
                        <clip>
                            <near>0.1</near>
                            <far>100</far>
                        </clip>
                    </camera>
                    <always_on>1</always_on>
                    <update_rate>30</update_rate>
                    <visualize>true</visualize>
                    <topic>/camera/camera/infra1/image_rect_raw</topic>
                </sensor>
            </link>
            
            <!-- Infra2 Camera Link (Right) -->
            <link name='infra2_link'>
                <pose relative_to="chassis">1.0 -0.0255 0.4 0 0 0</pose>
                <inertial>
                    <mass>0.1</mass>
                    <inertia>
                        <ixx>0.001</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
